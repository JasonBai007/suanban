{
  "presets": [
    ["latest", {
      "es2015": { "modules": false }
    }],
    "stage-2"
  ],
  "plugins": [
    "transform-runtime",
    [
      "component", [
          {
            "libraryName": "mint-ui",
            "style": true
          }
        ]
    ]
  ],
  "comments": false,
  "env": {
    "test": {
      "presets": ["latest", "stage-2"],
      "plugins": [ "istanbul" ]
    }
  }
}
